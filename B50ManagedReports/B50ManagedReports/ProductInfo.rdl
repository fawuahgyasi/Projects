        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="ProdSubCatvals">
      <Query>
        <DataSourceName>AWDWR2</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ProdCat">
            <Value>=Parameters!ProdCat.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT Distinct EnglishProductSubcategoryName
FROM DimProductSubcategory PS
INNER JOIN DimProductCategory PC
ON PS.ProductCategoryKey= PC.ProductCategoryKey
AND PC.EnglishProductCategoryName = @ProdCat</CommandText>
      </Query>
      <Fields>
        <Field Name="EnglishProductSubcategoryName">
          <DataField>EnglishProductSubcategoryName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
