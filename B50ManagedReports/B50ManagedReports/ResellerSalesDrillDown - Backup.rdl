  <DataSets>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="SalesTerritoryGroup">
          <DataField>SalesTerritoryGroup</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Country">
          <DataField>Country</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Reseller">
          <DataField>Reseller</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ProductCategory">
          <DataField>ProductCategory</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Qty">
          <DataField>Qty</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="SalesAmount">
          <DataField>SalesAmount</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AWDWR2</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT        DST.SalesTerritoryGroup, DST.SalesTerritoryCountry AS Country, DR.ResellerName AS Reseller, PC.EnglishProductCategoryName AS ProductCategory, 
                         SUM(FRS.OrderQuantity) AS Qty, SUM(FRS.SalesAmount) AS SalesAmount
FROM            DimProduct AS P INNER JOIN
                         DimProductSubcategory AS PS ON P.ProductSubcategoryKey = PS.ProductSubcategoryKey INNER JOIN
                         DimProductCategory AS PC ON PC.ProductCategoryKey = PS.ProductCategoryKey INNER JOIN
                         FactResellerSales AS FRS ON FRS.ProductKey = P.ProductKey INNER JOIN
                         DimReseller AS DR ON DR.ResellerKey = FRS.ResellerKey INNER JOIN
                         DimGeography AS DG ON DG.GeographyKey = DR.GeographyKey INNER JOIN
                         DimSalesTerritory AS DST ON DST.SalesTerritoryKey = DG.SalesTerritoryKey
GROUP BY DST.SalesTerritoryGroup, DST.SalesTerritoryCountry, DR.ResellerName, PC.EnglishProductCategoryName</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
  </DataSets>
