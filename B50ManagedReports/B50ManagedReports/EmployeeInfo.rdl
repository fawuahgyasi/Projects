  <DataSets>
    <DataSet Name="EmpContactInfo">
      <Query>
        <DataSourceName>AWDB</DataSourceName>
        <CommandText>SELECT        HRE.EmployeeID, PC.FirstName + ' ' + ISNULL(PC.MiddleName, N' ') + ' ' + PC.LastName AS [Full Name], HRE.Title, PA.AddressLine1, PA.City, PA.PostalCode, 
                         PSP.Name AS State, PCR.Name AS Country, PC.Phone
FROM            HumanResources.Employee AS HRE INNER JOIN
                         Person.Contact AS PC ON PC.ContactID = HRE.ContactID INNER JOIN
                         HumanResources.EmployeeAddress AS HREA ON HRE.EmployeeID = HREA.EmployeeID INNER JOIN
                         Person.Address AS PA ON PA.AddressID = HREA.AddressID INNER JOIN
                         Person.StateProvince AS PSP ON PSP.StateProvinceID = PA.StateProvinceID INNER JOIN
                         Person.CountryRegion AS PCR ON PCR.CountryRegionCode = PSP.CountryRegionCode</CommandText>
      </Query>
      <Fields>
        <Field Name="EmployeeID">
          <DataField>EmployeeID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Full_Name">
          <DataField>Full Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Title">
          <DataField>Title</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="AddressLine1">
          <DataField>AddressLine1</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="City">
